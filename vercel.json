{
    "rewrites": [
        {
            "source": "/api/debug",
            "destination": "https://httpbin.org/get"
        },
        {
            "source": "/api/:match*",
            "destination": "https://api.webflow.com/:match*"
        },
        {
            "source": "/tinify/:match*",
            "destination": "https://api.tinify.com/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}